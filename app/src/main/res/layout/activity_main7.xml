<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity7">

    <TextView
        android:id="@+id/textTransport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="優先ネットワーク"
        android:textSize="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/transport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="example"
        android:textSize="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/textTransport"
        app:layout_constraintStart_toEndOf="@id/textTransport" />
    <TextView
        android:id="@+id/textIP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IPアドレス"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@id/textTransport"
        app:layout_constraintTop_toBottomOf="@id/textTransport" />
    <TextView
        android:id="@+id/IPadress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@id/textIP"
        app:layout_constraintTop_toBottomOf="@id/textIP" />
</androidx.constraintlayout.widget.ConstraintLayout>